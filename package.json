{
  "name": "webpack-starter",
  "version": "0.0.1",
  "description": "sample webpack starter with React",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "start": "http-server src",
    "dev": "webpack-dev-server --env.dev",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "validate": "npm-run-all --parallel lint build",
    "lint": "eslint src"
  },
  "author": "Eric Ren",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.1.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.20",
    "webpack-config-utils": "^2.1.0",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
